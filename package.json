{
  "name": "knows",
  "version": "1.0.1",
  "description": "A minimal cross-platform CLI that knows what’s running — list, inspect, and kill processes easily.",
  "bin": {
    "knows": "./dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "node scripts/build.mjs",
    "build:tsc": "tsc -p tsconfig.json",
    "dev": "ts-node src/index.ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "cli",
    "process",
    "ports",
    "utility"
  ],
  "author": "minagishl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/minagishl/knows.git"
  },
  "homepage": "https://github.com/minagishl/knows#readme",
  "bugs": {
    "url": "https://github.com/minagishl/knows/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "find-process": "^1.4.7",
    "inquirer": "^9.2.10"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.10.0",
    "esbuild": "^0.25.11",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}
